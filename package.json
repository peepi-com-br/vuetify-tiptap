{
  "name": "@peepi/vuetify-tiptap",
  "version": "0.1.4",
  "license": "MIT",
  "description": "Awesome and extendable rich text editor component for Vuetify projects using tiptap",
  "homepage": "https://github.com/peepi-com-br/vuetify-tiptap",
  "repository": {
    "type": "git",
    "url": "https://github.com/peepi-com-br/vuetify-tiptap.git"
  },
  "bugs": {
    "url": "https://github.com/peepi-com-br/vuetify-tiptap/issues"
  },
  "keywords": [
    "vue",
    "vuetify",
    "tiptap",
    "richtext",
    "wysiwyg"
  ],
  "main": "dist/v-tiptap.ssr.js",
  "browser": "dist/v-tiptap.esm.js",
  "module": "dist/v-tiptap.esm.js",
  "unpkg": "dist/v-tiptap.min.js",
  "types": "dist/types/src/entry.esm.d.ts",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": [
    "*.vue"
  ],
  "scripts": {
    "serve": "cross-env DEMO_ENV=development vue-cli-service serve",
    "build:demo": "cross-env DEMO_ENV=production vue-cli-service build",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "postbuild": "rimraf ./dist/types/dev ./dist/types/src/entry.d.ts",
    "prepublishOnly": "npm run build",
    "storybook:build": "vue-cli-service storybook:build -c .storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c .storybook",
    "pretty": "prettier --write \"./**/*.{js,ts,jsx,json,vue}\""
  },
  "dependencies": {
    "@tiptap/vue-2": "^2.0.0-beta.77",
    "v-emoji-picker": "^2.3.3",
    "xss": "^1.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@mdi/font": "^6.6.96",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-interactions": "^6.4.21",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/jest": "^0.0.10",
    "@storybook/testing-library": "^0.0.9",
    "@storybook/vue": "^6.0.26",
    "@tiptap/extension-character-count": "^2.0.0-beta.25",
    "@tiptap/extension-color": "^2.0.0-beta.9",
    "@tiptap/extension-focus": "^2.0.0-beta.40",
    "@tiptap/extension-highlight": "^2.0.0-beta.33",
    "@tiptap/extension-image": "^2.0.0-beta.27",
    "@tiptap/extension-link": "^2.0.0-beta.36",
    "@tiptap/extension-mention": "^2.0.0-beta.95",
    "@tiptap/extension-placeholder": "^2.0.0-beta.48",
    "@tiptap/extension-task-item": "^2.0.0-beta.31",
    "@tiptap/extension-task-list": "^2.0.0-beta.26",
    "@tiptap/extension-text-align": "^2.0.0-beta.29",
    "@tiptap/extension-text-style": "^2.0.0-beta.23",
    "@tiptap/extension-underline": "^2.0.0-beta.23",
    "@tiptap/starter-kit": "^2.0.0-beta.183",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^5.0.1",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "cache-loader": "^4.1.0",
    "css-loader": "^5.0.0",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "delay": "^5.0.0",
    "eslint": "^8.10.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "jsdom-global": "^3.0.2",
    "minimist": "^1.2.5",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "roboto-fontface": "^0.10.0",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "rollup-plugin-vue": "^5.1.9",
    "rollup-plugin-vuetify": "^0.2.4",
    "sass": "~1.32",
    "sass-loader": "^10.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.3",
    "vue": "^2.6.14",
    "vue-cli-plugin-storybook": "~2.1.0",
    "vue-cli-plugin-vuetify": "~2.4.6",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.6.3",
    "vuetify-loader": "^1.7.3",
    "webpack": "^4.45.0"
  },
  "peerDependencies": {
    "vue": "^2.6.14",
    "vuetify": "^2.6.3"
  },
  "engines": {
    "node": ">=12"
  }
}
